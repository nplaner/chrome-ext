{
    "manifest_version": 2,
    "name": "Kill Switch",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery.min.js", 
                "content.js"]
        }
      ],
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png"
      }
  }
